{
  "name": "nuber-eats-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tailwind:build": "tailwind build ./src/styles/tailwind.css -o ./src/styles/styles.css",
    "start": "yarn generate & yarn tailwind:build & cross-env DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:vbo": "react-scripts test --verbose",
    "eject": "react-scripts eject",
    "generate": "rimraf src/__generated__ & apollo client:codegen src/__generated__ --target=typescript --outputFlat",
    "test:cov": "yarn test --coverage --watchAll=false",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "apollo": "^2.33.1",
    "autoprefixer": "^10.2.5",
    "graphql": "^15.5.0",
    "postcss": "^8.2.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.0.9",
    "react-hook-form": "^7.4.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "cross-env": "^7.0.3",
    "cypress": "^7.4.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mock-apollo-client": "^1.1.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/components/**/*.tsx",
      "./src/pages/**/*.tsx",
      "./src/routers/**/*.tsx"
    ]
  }
}
